{
  "name": "angular-sortable-items",
  "version": "1.0.0",
  "description": "angular application which allow you to sort lists of items",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080 --open",
    "build": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./src/app/**/*.ts -t verbose",
    "pree2e": "webdriver-manager update",
    "test": "karma start",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "keywords": [
    "angular"
  ],
  "author": "pure-js",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "angular-in-memory-web-api": "~0.2.4",
    "core-js": "^2.4.1",
    "octicons": "^5.0.1",
    "rxjs": "5.0.3",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.36",
    "@types/node": "^7.0.0",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "bootstrap": "^4.0.0-alpha.6",
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "~2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "lodash": "^4.16.4",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "~5.0.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "tslint": "^4.3.1",
    "typescript": "~2.1.5",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^2.4.0"
  },
  "repository": {}
}
